<Window x:Class="PayrollSystem.UI.SetWorkedHoursWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Задать кол-во отработанных часов" Height="112" Width="280" MinHeight="112" MinWidth="280">
    <Grid>
        <StackPanel>
            <TextBox x:Name="WorkedHoursTextBox" Margin="15 5 15 5" FontSize="15"></TextBox>
            <Button Click="SetHoursClick" Content="Задать часы" FontSize="15" Margin="15 5 15 5"></Button>
        </StackPanel>
    </Grid>
</Window>
